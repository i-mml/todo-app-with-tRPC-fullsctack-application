{
  "name": "examples-minimal-react-server",
  "private": true,
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch index.ts",
    "lint": "eslint --cache index.ts",
    "start": "node dist/index.js",
    "exec": "nodemon --watch \"src/**\" --ext \"ts,json\" --ignore \"src/**/*.spec.ts\" --exec \"ts-node src/index.ts\""
  },
  "dependencies": {
    "@trpc/server": "npm:@trpc/server@next",
    "concurrently": "^9.1.0",
    "cors": "^2.8.5",
    "express": "^4.21.1",
    "mongoose": "^8.8.3",
    "ts-node": "^10.9.2",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^5.0.0",
    "@types/node": "^22.9.0",
    "eslint": "^9.13.0",
    "nodemon": "^3.1.7",
    "tsx": "^4.0.0",
    "typescript": "^5.7.2"
  }
}
